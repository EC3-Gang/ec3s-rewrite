{
	"extends": "astro/tsconfigs/base",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strictNullChecks": true,
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/components/layouts/*"],
			"@styles/*": ["src/styles/*"],
			"@assets/*": ["src/assets/*"],
			"@utils/*": ["src/utils/*"],
		}
	}
}
